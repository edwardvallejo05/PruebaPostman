			"name": "Create User",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const jsonData = pm.response.json()",
							"",
							"pm.collectionVariables.set('userID', jsonData.data.id)",
							"",
							"pm.test(\"Status test 201\", function () {",
							"    pm.response.to.have.status(201);",
							"});",
							"",
							"pm.test(\"User Names\", function () {",
							"    pm.expect(jsonData.data.name).to.be.equal('Edward Vallejo');",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
